# Plausible Analytics Environment Variables
# Copy this file to .env and fill in your actual values

# Domain where Plausible will be accessible
BASE_URL=http://your-domain.com

# Secret key for encryption (64 bytes base64)
# CRITICAL: Use your existing value from Dokploy, do NOT regenerate!
SECRET_KEY_BASE=your-secret-key-base-here

# TOTP vault key for 2FA (32 bytes base64)
# CRITICAL: Use your existing value from Dokploy, do NOT regenerate!
TOTP_VAULT_KEY=your-totp-vault-key-here

# Optional: Add any other env vars you have in Dokploy
# DISABLE_REGISTRATION=true
# MAILER_EMAIL=
# SMTP_HOST_ADDR=
